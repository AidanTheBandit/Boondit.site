---
type Props = {
  title: string;
};
const { title = "Title" } = Astro.props;
---

<div
  class="bg-accent flex items-center justify-center leading-none pt-2 pb-1 text-center select-none rounded-2xl px-3 md:px-5 font-semibold text-accent-foreground w-fit text-sm md:text-base"
>
  <span>{title}</span>
</div>
